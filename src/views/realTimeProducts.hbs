
<h1> Real Time product page</h1>


<br>
<br>

<div id="tabla">
    <table  class="table">
        <thead>
            <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            </tr>
        </thead>
        <tbody>
            {{#each products}}
                <tr>
                    <td >{{this._id}}</td>
                    <td> {{this.title}} </td>
                    <td>  {{this.price}}</td>
            
                </tr>
              {{/each}}
        </tbody>
    </table>

</div>





<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducts.js"></script>